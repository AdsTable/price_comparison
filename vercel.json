{
    "version": 2,
    "builds": [
      {
        "src": "price_comparision/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config":{"disDir": "staticfiles_build"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "price_comparision/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
}